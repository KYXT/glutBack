                        "title": "email",
                        "description": "Email",
                        "type": "string",
                        "example": "admin@admin.com"
                    },
                    "password": {
                        "title": "passsword",
                        "description": "Password",
                        "type": "string",
                        "example": "adminadmin"
                    }
                },
                "type": "object"
            },
            "RegisterRequest": {
                "title": "RegisterRequest",
                "description": "Class RegisterRequest",
                "required": [
                    "name",
                    "email",
                    "password"
                ],
                "properties": {
                    "name": {
                        "title": "name",
                        "description": "User's name",
                        "type": "string",
                        "example": "KYXT"
                    },
                    "email": {
                        "title": "email",
                        "description": "User's email",
                        "type": "string",
                        "example": "user@user.com"
                    },
                    "password": {
                        "title": "password",
                        "description": "User's password",
                        "type": "string",
                        "example": "useruser"
                    }
                },
                "type": "object"
            },
            "UserRequest": {
                "title": "UserRequest",
                "description": "Class UserRequest",
                "properties": {
                    "name": {
                        "title": "name",
                        "description": "name",
                        "type": "string",
                        "example": "Peter"
                    },
                    "email": {
                        "title": "email",
                        "description": "Email",
                        "type": "string",
                        "example": "admin@admin.com"
                    },
                    "email_verified_at": {
                        "title": "email_verified_at",
                        "description": "Time whe email was verified",
                        "type": "string",
                        "example": "2021-08-21T00:07:55.000000Z"
                    },
                    "role": {
                        "title": "Role",
                        "description": "User's role",
                        "type": "integer",
                        "example": "1"
                    },
                    "created_at": {
                        "title": "created_at",
                        "description": "Time whe uese was created",
                        "type": "string",
                        "example": "2021-08-21T00:07:55.000000Z"
